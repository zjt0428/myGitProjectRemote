SET @ymds = CONVERT(varchar(8),@ym_beg,112)
SET @ymde = CONVERT(varchar(8),@ym_end,112)
	select (select value from BM_COMPONENT_GENERIC where CODE=COMPON_GENERIC)COMPON_GENERICC,
	SUM(quantity)quantity from T_PICKUP_COMPONENT where (COMPON_GENERIC
