.thumb-wrap {
	width: 31%;
	border: 1px solid #c7e3fa;
	float: left;
	margin: 4px;
	margin-right: 0;
	padding: 4px;
}

.thumb-wrap span {
	display: block;
	margin: 1px;
	overflow: hidden;
}

.thumb-wrap span font {
	color: #2A4D8F;
	font-weight: bolder;
	font-size: 12px;
}

.x-view-over {
	border: 1px solid #ddd;
	background: #efefef;
	padding: 4px;
}

.x-view-selected {
	background: #DFEDFF;
	border: 1px solid #6593cf;
	padding: 4px;
}

.x-view-selected .thumb {
	background: transparent;
}

.x-view-selected span {
	color: #1A4D8F;
}

