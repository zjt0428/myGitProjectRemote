body {
	font-size: 12px;
	font-family: '宋体';
}

.clear {
	clear: both;
	margin: 0;
	padding: 0;
	border: 0;
}

#loading-mask {
	position: absolute;
	left: 0;
	top: 0;
	width: 100%;
	height: 100%;
	z-index: 20000;
	background-color: white;
}

#loading {
	position: absolute;
	left: 45%;
	top: 40%;
	padding: 2px;
	z-index: 20001;
	height: auto;
}

#loading a {
	color: #258;
}

#loading .loading-indicator {
	background: white;
	color: #444;
	font: bold 13px tahoma, arial, helvetica;
	padding: 10px;
	margin: 0;
	height: auto;
}

#loading-msg {
	font: normal 14px arial, tahoma, sans-serif;
	color: blue;
	padding-top: 18px;
}

div button {
	border: none;
	cursor: pointer;
	width: 16px;
	height: 16px;
}

