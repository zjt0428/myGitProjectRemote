<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.knight.emms.model.ClosedSettleInfo" table="T_CLOSED_SETTLE_INFO" select-before-update="false" dynamic-update="false" dynamic-insert="false"
	optimistic-lock="version">
		<id name="closeSettleId" column="CLOSE_SETTLE_ID" type="java.lang.Long" length="19">
			<generator class="native" />
		</id>
		<property name="closedDate" type="java.lang.String" column="CLOSED_DATE" />
		<property name="closedStatus" type="java.lang.String" column="CLOSED_STATUS" />
		<property name="months" type="java.lang.String" column="MONTHS" />
	</class>
</hibernate-mapping>
