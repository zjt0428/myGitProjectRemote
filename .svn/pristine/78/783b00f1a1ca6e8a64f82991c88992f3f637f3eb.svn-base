	<queryString>
		<![CDATA[DECLARE @leaseUnit VARCHAR(128)
DECLARE @paEntName VARCHAR(128)
DECLARE @competentDepartment VARCHAR(128)
DECLARE @address VARCHAR(64)
DECLARE @contractSerial VARCHAR(64)
DECLARE @startDate VARCHAR(10)
DECLARE @endDate VARCHAR(10)

SET @contractSerial = $P{contractSerial}
SET @leaseUnit = $P{leaseUnit}
SET @paEntName = $P{paEntName}
SET @competentDepartment = $P{competentDepartment}
SET @address = $P{address}
SET @startDate = $P{startDate}
SET @endDate = $P{endDate}

SELECT
TCM.CONTRACT_SERIAL,
TCM.PA_ENT_NAME,
TCM.PROJECT_NAME,
BP.[VALUE]+BC.[VALUE] AS 'ADDRESS',
TCM.COMPETENT_DEPARTMET,
TCM.TAX_RATE AS 'TAX_RATE_MATERIALS',
T.*
FROM
(
	SELECT
	(SELECT REPLACE(SUBSTRING(TLS.END_DATE,0,8),'-','')) as 'YEAR_MONTH',
	TLC.LEASE_IDENTIFIER,
	TLC.CONTRACT_ID,
	TLC.LEASE_UNIT,
	CONVERT(DECIMAL(15,2),ISNULL(TLS.CURRENT_SETTLEMENT_AMOUNT,0)) AS 'CURRENT_SETTLEMENT_AMOUNT',	--含税成本/本期结算金额
	TLC.TAX_RATE AS 'TAX_RATE_LEASE',  --租借合同税率
	TLC.AUTHORIZED_AMOUNT,  --可抵税率
	CONVERT(DECIMAL(15,2),ISNULL(TLS.INSIDE_SETTLEMENT_AMOUNT,0)) AS 'INSIDE_SETTLEMENT_AMOUNT' --含税产值/内部结算金额
	FROM T_LEASE_SETTLEMENT TLS,T_LEASE_CONTRACT TLC
	WHERE TLC.LEASE_ID=TLS.LEASE_ID AND TLS.DEL_FLAG=1 AND TLS.STATUS=3
	AND (TLC.LEASE_UNIT LIKE '%'+@leaseUnit+'%' OR @leaseUnit='' OR @leaseUnit IS NULL)
	AND TLS.START_DATE>=@startDate AND TLS.END_DATE<=@endDate
	AND ($P{contractIds} IS NULL OR $P{contractIds}='' OR TLC.CONTRACT_ID IN (SELECT * FROM dbo.Split($P{contractIds},',')))
) T
LEFT JOIN T_CONTRACT_MATERIALS TCM ON TCM.CONTRACTMA_ID=T.CONTRACT_ID
LEFT JOIN T_PROJECT TP ON TP.PROJECT_ID = TCM.PROJECT_ID
LEFT JOIN BM_PROVINCE BP ON BP.CODE=TP.PROVINCE
LEFT JOIN BM_CITY BC ON BC.CODE=TP.CITY
WHERE (TCM.PA_ENT_NAME LIKE '%'+@paEntName+'%' OR @paEntName='' OR @paEntName IS NULL)
AND (TCM.COMPETENT_DEPARTMET LIKE '%'+@competentDepartment+'%' OR @competentDepartment='' OR @competentDepartment IS NULL)
AND (BP.[VALUE]+BC.[VALUE] LIKE '%'+@address+'%' OR @address='' OR @address IS NULL)
AND (TCM.CONTRACT_SERIAL LIKE '%'+@contractSerial+'%' OR @contractSerial='' OR @contractSerial IS NULL)]]>
	</queryString>
