<center>
<div class="hzbmain_detail">
<p style="text-align:center; "><span class="wrod_title">机  械  设  备  月  租  金  结  算  单</span></p>
<p style="text-align:center; padding-bottom:10px;"><span><b>（${titleDate}）</b></span></p>
<table width="100%" border="0" cellspacing="0" cellpadding="0">
	<tr>
		<td height="26" width="90px" style=" text-align:right;">承租单位：</td>
		<td height="26" >${settleContract.paEntName}</td>
	</tr>
	<tr>
		<td width="90px" style="text-align:right;border-left-style:none;">项目名称：</td>
		<td colspan="3" height="26" >${settleContract.projectName}</td>
		<td height="26" style="padding-left:5px; text-align:right;">结算日期：${settleDate}</td>
	</tr>
</table>
<table width="100%" border="0" cellspacing="0" cellpadding="0" class="hzb_table">
	<tr style="background-color:#E0E0E0; font-size: 10px;">
		<td rowspan="2" width="3%">序号</td>
		<td rowspan="2" width="4%">楼号</td>
		<td rowspan="2" width="5%">设备名称及型号</td>
		<!-- <td rowspan="2" width="5%">规格型号</td> -->
		<td rowspan="2" width="5%">合同编号</td>
		<td rowspan="2" width="5%">起租日期</td>
		<td rowspan="2" width="8%">结算区间</td>
		<td rowspan="2" width="8%">本期使用天数</td>
		<td colspan="2" width="8%">月租金</td>
		<td colspan="1" width="8%">进退场费用</td>
		<td colspan="1" width="8%">操作人员工资</td>
		<td colspan="1" width="8%">安全监控系统</td>
		<c:if test="${totalSummaryCompon!='0'}">
		<td colspan="1" width="8%">加节费用</td>
		</c:if>
		<c:if test="${totalSummaryOther!='0'}">
		<td colspan="1" width="8%">其他费用</td>
		</c:if>
		<!-- <td width="5%">伙食补贴</td> -->
		<td rowspan="2" width="5%">结算金额小计</td>
	</tr>
	<tr style="background-color:#E0E0E0; font-size: 10px;" >
		<!-- <td >台数</td> -->
		<td >单价</td>
		<td >金额</td>
		<!-- <td >台数</td>
		<td >单价</td> -->
		<td >金额</td>
		<!-- <td >台数</td>
		<td >单价</td> -->
		<td >金额</td>
		<!-- <td >台数</td>
		<td >单价</td> -->
		<td >金额</td>
		<!-- <td >台数</td>
		<td >单价</td> -->
		<c:if test="${totalSummaryCompon!='0'}">
		<td >金额</td>
		</c:if>
		<!-- <td >台数</td>
		<td >单价</td> -->
		<c:if test="${totalSummaryOther!='0'}">
		<td >金额</td>
		</c:if>
		<!-- <td >金额</td> -->
	</tr>
	<c:forEach var="element" items="${settleEquipBrief}" varStatus="status">
	<tr style="font-size: 10px;">
		<td >${status.count}</td>
		<td >${element["buildingNum"]}</td>
		<td >${element["equipCategoryName"]}</br>${element["equipSpecificName"]}</td>
		<%-- <td >${element["equipSpecificName"]}</td> --%>
		<%-- <c:if test="${status.count==1}"> --%>
		<td >${contractNo}</td>
		<td >${element["activateDate"]}</td>
		<td >${element["interval"]}</td>
		<%-- </c:if> --%>
		<td >${element["settleDays"]}</td>
		<%-- <td >${element["quantityEquip"]}</td> --%>
		<td >${element["rentStandard"]}</td>
		<td >${element["summaryEquip"]}</td>
		<%-- <td >${element["quantityItem"]}</td>
		<td >${element["unitpriceItem"]}</td> --%>
		<td >${element["summaryItem"]}</td>
		<%-- <td >${element["quantityOperator"]}</td>
		<td >${element["unitpriceOperator"]}</td> --%>
		<td >${element["costTotal"]}</td>
		<%-- <td >${element["quantitySafety"]}</td>
		<td >${element["unitpriceSafety"]}</td> --%>
		<td >${element["summarySafety"]}</td>
		<%-- <td >${element["quantityCompon"]}</td>
		<td >${element["unitpriceCompon"]}</td> --%>
		<c:if test="${totalSummaryCompon!='0'}">
		<td >${element["summaryCompon"]}</td>
		</c:if>
		<%-- <td >${element["quantityOther"]}</td>
		<td >${element["unitpriceOther"]}</td> --%>
		<c:if test="${totalSummaryOther!='0'}">
		<td >${element["summaryOther"]}</td>
		</c:if>
		<%-- <td >${element["foodAllowance"]}</td> --%>
		<td >${element["summaryEquip"]+element["summaryItem"]+element["costTotal"]+element["summaryCompon"]+element["summarySafety"]+element["summaryOther"]}</td>
	</tr>
	</c:forEach>
	<tr style="font-size: 10px;">
		<td colspan="2">合计</td>
		<td colspan="5">0</td>
		<!-- <td ></td> -->
		<!-- <td ></td>
		<td ></td>
		<td ></td> -->
		<!-- <td ></td> -->
		<!-- <td ></td> -->
		<td >0</td>
		<td >${totalSummaryEquip}</td>
		<!-- <td ></td>
		<td ></td> -->
		<td >${totalSummaryItem}</td>
		<!-- <td ></td>
		<td ></td> -->
		<td >${totalCostTotal}</td>
		<!-- <td ></td>
		<td ></td> -->
		<td >${totalSummarySafety}</td>
		<!-- <td ></td>
		<td ></td> -->
		<c:if test="${totalSummaryCompon!='0'}">
		<td >${totalSummaryCompon}</td>
		</c:if>
		<!-- <td ></td>
		<td ></td> -->
		<c:if test="${totalSummaryOther!='0'}">
		<td >${totalSummaryOther}</td>
		</c:if>
		<%-- <td >${totalFoodAllowance}</td> --%>
		<td >${totalSummaryEquip+totalSummaryItem+totalCostTotal+totalSummarySafety+totalSummaryOther+totalSummaryCompon}</td>
	</tr>
	<tr style="font-size: 10px;">
		<td width="8%" colspan="2">备注</td>
		<%-- <td width="8%" colspan="11">${settleContract.remark}</td> --%>
		<c:choose>
			<c:when test="${totalSummaryCompon!='0' && totalSummaryOther!='0'}"><td width="8%" colspan="13">${settleContract.remark}</td></c:when>
			<c:when test="${totalSummaryCompon!='0' && totalSummaryOther=='0'}"><td width="8%" colspan="12">${settleContract.remark}</td></c:when>
			<c:when test="${totalSummaryCompon=='0' && totalSummaryOther!='0'}"><td width="8%" colspan="12">${settleContract.remark}</td></c:when>
			<c:when test="${totalSummaryCompon=='0' && totalSummaryOther=='0'}"><td width="8%" colspan="11">${settleContract.remark}</td></c:when>
		</c:choose>
	</tr>
</table>
<%-- <table style="width: 100%" class="hzb_table">   
	<tr style="background-color:#E0E0E0;">
		<td width="8%" colspan="1">备注</td>
		<td width="79%" colspan="2">${settleContract.remark}</td>
	</tr>
</table> --%>

<table style="width: 100%" class="hzb_table">   
	<tr style="background-color:#E0E0E0; font-size: 10px;">
		<td width="8%" colspan="2">累计结算金额（元）</td>
		<td width="8%" colspan="2">${settleContract.summaryReceivable}</td>
		<td width="8%" colspan="2">累计付款金额（元）</td>
		<td width="8%" colspan="2">${settleContract.receivedAmount}</td>
		<td width="8%" colspan="2">累计欠款金额（元）</td>
		<td width="8%" colspan="2">${settleContract.arrears}</td>
		<td width="8%" colspan="4">&nbsp;</td>
	</tr>
</table>

