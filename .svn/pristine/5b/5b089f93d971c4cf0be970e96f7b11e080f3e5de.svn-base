<?xml version="1.0" encoding="UTF-8"?>
<serviceGroup>
	<service name="TerminalService">
		<description>TerminalService:Spring POJO Axis2 Service Sample</description>
		<parameter name="ServiceClass">com.knight.emms.terminal.TerminalProxyService</parameter>
		<parameter name="ServiceObjectSupplier">
			org.apache.axis2.extensions.spring.receivers.SpringServletContextObjectSupplier
		</parameter>
		<parameter name="SpringBeanName">terminalProxyService</parameter>
		<messageReceivers>
			<messageReceiver mep="http://www.w3.org/2004/08/wsdl/in-out" class="org.apache.axis2.rpc.receivers.RPCMessageReceiver" />
		</messageReceivers>
	</service>
</serviceGroup>   
