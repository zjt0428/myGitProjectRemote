	<queryString>
		<![CDATA[DECLARE @YEARMTH VARCHAR(24)
DECLARE @YEARMTH_LAST VARCHAR(24)
DECLARE @DEP_NAME VARCHAR(64)
DECLARE @FULLNAME VARCHAR(24)

SET @YEARMTH = $P{YEARMTH}
SET @YEARMTH_LAST = $P{YEARMTH_LAST}
SET @DEP_NAME =  $P{DEP_NAME}
SET @FULLNAME = $P{FULL_NAME}
SELECT * FROM(
--签到
SELECT USER_SERIAL,FULLNAME,
	   '01' ROW,
	   SUBSTRING(MAX(_21Th),0,6) _21Th,
	   SUBSTRING(MAX(_22Th),0,6) _22Th,
	   SUBSTRING(MAX(_23Th),0,6) _23Th,
	   SUBSTRING(MAX(_24Th),0,6) _24Th,
	   SUBSTRING(MAX(_25Th),0,6) _25Th,
	   SUBSTRING(MAX(_26Th),0,6) _26Th,
	   SUBSTRING(MAX(_27Th),0,6) _27Th,
	   SUBSTRING(MAX(_28Th),0,6) _28Th,
	   SUBSTRING(MAX(_29Th),0,6) _29Th,
	   SUBSTRING(MAX(_30Th),0,6) _30Th,
	   SUBSTRING(MAX(_31Th),0,6) _31Th,
	   SUBSTRING(MAX(_01Th),0,6) _01Th,
	   SUBSTRING(MAX(_02Th),0,6) _02Th,
	   SUBSTRING(MAX(_03Th),0,6) _03Th,
	   SUBSTRING(MAX(_04Th),0,6) _04Th,
	   SUBSTRING(MAX(_05Th),0,6) _05Th,
	   SUBSTRING(MAX(_06Th),0,6) _06Th,
	   SUBSTRING(MAX(_07Th),0,6) _07Th,
	   SUBSTRING(MAX(_08Th),0,6) _08Th,
	   SUBSTRING(MAX(_09Th),0,6) _09Th,
	   SUBSTRING(MAX(_10Th),0,6) _10Th,
	   SUBSTRING(MAX(_11Th),0,6) _11Th,
	   SUBSTRING(MAX(_12Th),0,6) _12Th,
     SUBSTRING(MAX(_13Th),0,6) _13Th,
	   SUBSTRING(MAX(_14Th),0,6) _14Th,
	   SUBSTRING(MAX(_15Th),0,6) _15Th,
	   SUBSTRING(MAX(_16Th),0,6) _16Th,
	   SUBSTRING(MAX(_17Th),0,6) _17Th,
	   SUBSTRING(MAX(_18Th),0,6) _18Th,
	   SUBSTRING(MAX(_19Th),0,6) _19Th,
	   SUBSTRING(MAX(_20Th),0,6) _20Th
FROM(
SELECT AU.USER_SERIAL,AU.FULLNAME,
		(SELECT top 1 A.SGIN_TIME WHERE A.SG_DATE LIKE @YEARMTH_LAST+'-21') _21Th,
		(SELECT top 1 A.SGIN_TIME WHERE A.SG_DATE LIKE @YEARMTH_LAST+'-22') _22Th,
		(SELECT top 1 A.SGIN_TIME WHERE A.SG_DATE LIKE @YEARMTH_LAST+'-23') _23Th,
		(SELECT top 1 A.SGIN_TIME WHERE A.SG_DATE LIKE @YEARMTH_LAST+'-24') _24Th,
		(SELECT top 1 A.SGIN_TIME WHERE A.SG_DATE LIKE @YEARMTH_LAST+'-25') _25Th,
		(SELECT top 1 A.SGIN_TIME WHERE A.SG_DATE LIKE @YEARMTH_LAST+'-26') _26Th,
		(SELECT top 1 A.SGIN_TIME WHERE A.SG_DATE LIKE @YEARMTH_LAST+'-27') _27Th,
		(SELECT top 1 A.SGIN_TIME WHERE A.SG_DATE LIKE @YEARMTH_LAST+'-28') _28Th,
		(SELECT top 1 A.SGIN_TIME WHERE A.SG_DATE LIKE @YEARMTH_LAST+'-29') _29Th,
		(SELECT top 1 A.SGIN_TIME WHERE A.SG_DATE LIKE @YEARMTH_LAST+'-30') _30Th,
		(SELECT top 1 A.SGIN_TIME WHERE A.SG_DATE LIKE @YEARMTH_LAST+'-31') _31Th,
		(SELECT top 1 A.SGIN_TIME WHERE A.SG_DATE LIKE @YEARMTH+'-01') _01Th,
		(SELECT top 1 A.SGIN_TIME WHERE A.SG_DATE LIKE @YEARMTH+'-02') _02Th,
		(SELECT top 1 A.SGIN_TIME WHERE A.SG_DATE LIKE @YEARMTH+'-03') _03Th,
		(SELECT top 1 A.SGIN_TIME WHERE A.SG_DATE LIKE @YEARMTH+'-04') _04Th,
		(SELECT top 1 A.SGIN_TIME WHERE A.SG_DATE LIKE @YEARMTH+'-05') _05Th,
		(SELECT top 1 A.SGIN_TIME WHERE A.SG_DATE LIKE @YEARMTH+'-06') _06Th,
		(SELECT top 1 A.SGIN_TIME WHERE A.SG_DATE LIKE @YEARMTH+'-07') _07Th,
		(SELECT top 1 A.SGIN_TIME WHERE A.SG_DATE LIKE @YEARMTH+'-08') _08Th,
		(SELECT top 1 A.SGIN_TIME WHERE A.SG_DATE LIKE @YEARMTH+'-09') _09Th,
		(SELECT top 1 A.SGIN_TIME WHERE A.SG_DATE LIKE @YEARMTH+'-10') _10Th,
		(SELECT top 1 A.SGIN_TIME WHERE A.SG_DATE LIKE @YEARMTH+'-11') _11Th,
		(SELECT top 1 A.SGIN_TIME WHERE A.SG_DATE LIKE @YEARMTH+'-12') _12Th,
		(SELECT top 1 A.SGIN_TIME WHERE A.SG_DATE LIKE @YEARMTH+'-13') _13Th,
		(SELECT top 1 A.SGIN_TIME WHERE A.SG_DATE LIKE @YEARMTH+'-14') _14Th,
		(SELECT top 1 A.SGIN_TIME WHERE A.SG_DATE LIKE @YEARMTH+'-15') _15Th,
		(SELECT top 1 A.SGIN_TIME WHERE A.SG_DATE LIKE @YEARMTH+'-16') _16Th,
		(SELECT top 1 A.SGIN_TIME WHERE A.SG_DATE LIKE @YEARMTH+'-17') _17Th,
		(SELECT top 1 A.SGIN_TIME WHERE A.SG_DATE LIKE @YEARMTH+'-18') _18Th,
		(SELECT top 1 A.SGIN_TIME WHERE A.SG_DATE LIKE @YEARMTH+'-19') _19Th,
		(SELECT top 1 A.SGIN_TIME WHERE A.SG_DATE LIKE @YEARMTH+'-20') _20Th
FROM T_ATTENDAMCE A
LEFT JOIN APP_USER AU ON AU.USERID = A.USERID
LEFT JOIN DEPARTMENT D ON D.DEP_ID = AU.DEP_ID
WHERE (AU.FULLNAME LIKE '%'+@FULLNAME+'%' OR @FULLNAME IS NULL OR @FULLNAME='')
AND (D.DEP_NAME LIKE '%'+@DEP_NAME+'%' OR @DEP_NAME IS NULL OR @DEP_NAME='')
AND (A.SG_DATE LIKE @YEARMTH+'%' OR A.SG_DATE LIKE @YEARMTH_LAST+'%')
) B
GROUP BY B.FULLNAME,B.USER_SERIAL

UNION ALL
--签退
SELECT USER_SERIAL,FULLNAME,
	   '02' ROW,
	   SUBSTRING(MAX(_21Th),0,6) _21Th,
	   SUBSTRING(MAX(_22Th),0,6) _22Th,
	   SUBSTRING(MAX(_23Th),0,6) _23Th,
	   SUBSTRING(MAX(_24Th),0,6) _24Th,
	   SUBSTRING(MAX(_25Th),0,6) _25Th,
	   SUBSTRING(MAX(_26Th),0,6) _26Th,
	   SUBSTRING(MAX(_27Th),0,6) _27Th,
	   SUBSTRING(MAX(_28Th),0,6) _28Th,
	   SUBSTRING(MAX(_29Th),0,6) _29Th,
	   SUBSTRING(MAX(_30Th),0,6) _30Th,
	   SUBSTRING(MAX(_31Th),0,6) _31Th,
	   SUBSTRING(MAX(_01Th),0,6) _01Th,
	   SUBSTRING(MAX(_02Th),0,6) _02Th,
	   SUBSTRING(MAX(_03Th),0,6) _03Th,
	   SUBSTRING(MAX(_04Th),0,6) _04Th,
	   SUBSTRING(MAX(_05Th),0,6) _05Th,
	   SUBSTRING(MAX(_06Th),0,6) _06Th,
	   SUBSTRING(MAX(_07Th),0,6) _07Th,
	   SUBSTRING(MAX(_08Th),0,6) _08Th,
	   SUBSTRING(MAX(_09Th),0,6) _09Th,
	   SUBSTRING(MAX(_10Th),0,6) _10Th,
	   SUBSTRING(MAX(_11Th),0,6) _11Th,
	   SUBSTRING(MAX(_12Th),0,6) _12Th,
     SUBSTRING(MAX(_13Th),0,6) _13Th,
	   SUBSTRING(MAX(_14Th),0,6) _14Th,
	   SUBSTRING(MAX(_15Th),0,6) _15Th,
	   SUBSTRING(MAX(_16Th),0,6) _16Th,
	   SUBSTRING(MAX(_17Th),0,6) _17Th,
	   SUBSTRING(MAX(_18Th),0,6) _18Th,
	   SUBSTRING(MAX(_19Th),0,6) _19Th,
	   SUBSTRING(MAX(_20Th),0,6) _20Th
FROM(
SELECT AU.USER_SERIAL,AU.FULLNAME,
		(SELECT top 1 A.SGOU_TIME WHERE A.SG_DATE LIKE @YEARMTH_LAST+'-21') _21Th,
		(SELECT top 1 A.SGOU_TIME WHERE A.SG_DATE LIKE @YEARMTH_LAST+'-22') _22Th,
		(SELECT top 1 A.SGOU_TIME WHERE A.SG_DATE LIKE @YEARMTH_LAST+'-23') _23Th,
		(SELECT top 1 A.SGOU_TIME WHERE A.SG_DATE LIKE @YEARMTH_LAST+'-24') _24Th,
		(SELECT top 1 A.SGOU_TIME WHERE A.SG_DATE LIKE @YEARMTH_LAST+'-25') _25Th,
		(SELECT top 1 A.SGOU_TIME WHERE A.SG_DATE LIKE @YEARMTH_LAST+'-26') _26Th,
		(SELECT top 1 A.SGOU_TIME WHERE A.SG_DATE LIKE @YEARMTH_LAST+'-27') _27Th,
		(SELECT top 1 A.SGOU_TIME WHERE A.SG_DATE LIKE @YEARMTH_LAST+'-28') _28Th,
		(SELECT top 1 A.SGOU_TIME WHERE A.SG_DATE LIKE @YEARMTH_LAST+'-29') _29Th,
		(SELECT top 1 A.SGOU_TIME WHERE A.SG_DATE LIKE @YEARMTH_LAST+'-30') _30Th,
		(SELECT top 1 A.SGOU_TIME WHERE A.SG_DATE LIKE @YEARMTH_LAST+'-31') _31Th,
		(SELECT top 1 A.SGOU_TIME WHERE A.SG_DATE LIKE @YEARMTH+'-01') _01Th,
		(SELECT top 1 A.SGOU_TIME WHERE A.SG_DATE LIKE @YEARMTH+'-02') _02Th,
		(SELECT top 1 A.SGOU_TIME WHERE A.SG_DATE LIKE @YEARMTH+'-03') _03Th,
		(SELECT top 1 A.SGOU_TIME WHERE A.SG_DATE LIKE @YEARMTH+'-04') _04Th,
		(SELECT top 1 A.SGOU_TIME WHERE A.SG_DATE LIKE @YEARMTH+'-05') _05Th,
		(SELECT top 1 A.SGOU_TIME WHERE A.SG_DATE LIKE @YEARMTH+'-06') _06Th,
		(SELECT top 1 A.SGOU_TIME WHERE A.SG_DATE LIKE @YEARMTH+'-07') _07Th,
		(SELECT top 1 A.SGOU_TIME WHERE A.SG_DATE LIKE @YEARMTH+'-08') _08Th,
		(SELECT top 1 A.SGOU_TIME WHERE A.SG_DATE LIKE @YEARMTH+'-09') _09Th,
		(SELECT top 1 A.SGOU_TIME WHERE A.SG_DATE LIKE @YEARMTH+'-10') _10Th,
		(SELECT top 1 A.SGOU_TIME WHERE A.SG_DATE LIKE @YEARMTH+'-11') _11Th,
		(SELECT top 1 A.SGOU_TIME WHERE A.SG_DATE LIKE @YEARMTH+'-12') _12Th,
		(SELECT top 1 A.SGOU_TIME WHERE A.SG_DATE LIKE @YEARMTH+'-13') _13Th,
		(SELECT top 1 A.SGOU_TIME WHERE A.SG_DATE LIKE @YEARMTH+'-14') _14Th,
		(SELECT top 1 A.SGOU_TIME WHERE A.SG_DATE LIKE @YEARMTH+'-15') _15Th,
		(SELECT top 1 A.SGOU_TIME WHERE A.SG_DATE LIKE @YEARMTH+'-16') _16Th,
		(SELECT top 1 A.SGOU_TIME WHERE A.SG_DATE LIKE @YEARMTH+'-17') _17Th,
		(SELECT top 1 A.SGOU_TIME WHERE A.SG_DATE LIKE @YEARMTH+'-18') _18Th,
		(SELECT top 1 A.SGOU_TIME WHERE A.SG_DATE LIKE @YEARMTH+'-19') _19Th,
		(SELECT top 1 A.SGOU_TIME WHERE A.SG_DATE LIKE @YEARMTH+'-20') _20Th
FROM T_ATTENDAMCE A
LEFT JOIN APP_USER AU ON AU.USERID = A.USERID
LEFT JOIN DEPARTMENT D ON D.DEP_ID = AU.DEP_ID
WHERE (AU.FULLNAME LIKE '%'+@FULLNAME+'%' OR @FULLNAME IS NULL OR @FULLNAME='')
AND (D.DEP_NAME LIKE '%'+@DEP_NAME+'%' OR @DEP_NAME IS NULL OR @DEP_NAME='')
AND (A.SG_DATE LIKE @YEARMTH+'%' OR A.SG_DATE LIKE @YEARMTH_LAST+'%')
) B
GROUP BY B.FULLNAME,B.USER_SERIAL) C ORDER BY ROW]]>
	</queryString>
