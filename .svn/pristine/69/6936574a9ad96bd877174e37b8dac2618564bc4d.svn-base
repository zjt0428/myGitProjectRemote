<script type="text/javascript">
	$(function () {
		//首页提醒
        $.post("/emms/form/getIndexWarn.do", {}, function(res){
        	var obj =JSON.parse(res); 
        	if(obj.success){
        		var cur=obj.info.result;
        		$("#indexWarn_leaveEquip").empty().append(cur.leaveEquip);
        		$("#indexWarn_borrowEquip").empty().append(cur.borrowEquip);
        		$("#indexWarn_cancelEquip").empty().append(cur.cancelEquip);
        		$("#indexWarn_uselessEquip").empty().append(cur.uselessEquip);
        		$("#indexWarn_contract").empty().append(cur.contract);
        		$("#indexWarn_stockOut").empty().append(cur.stockOut);
        		$("#indexWarn_logisOut").empty().append(cur.logisOut);
        		$("#indexWarn_logisSign").empty().append(cur.logisSign);
        		$("#indexWarn_installForCheck").empty().append(cur.installForCheck);
        		$("#indexWarn_installForChecked").empty().append(cur.installForChecked);
        		$("#indexWarn_equipForDetect").empty().append(cur.equipForDetect);
        		$("#indexWarn_equipForAccept").empty().append(cur.equipForAccept);
        		$("#indexWarn_equipForUse").empty().append(cur.equipForUse);
        		$("#indexWarn_equipForRepair").empty().append(cur.equipForRepair);
        		$("#indexWarn_equipForPoll").empty().append(cur.equipForPoll);
        		$("#indexWarn_equipForKeep").empty().append(cur.equipForKeep);
        		$("#indexWarn_reform").empty().append(cur.reform);
        		$("#indexWarn_equipForQuip").empty().append(cur.equipForQuip);
        		$("#indexWarn_demount").empty().append(cur.demount);
        		$("#indexWarn_cutTo").empty().append(cur.cutTo);
        		$("#indexWarn_putExcep").empty().append(cur.putExcep);
        		$("#indexWarn_useedEquip").empty().append(cur.useedEquip);
        		$("#indexWarn_repairOut").empty().append(cur.repairOut);
        		$("#indexWarn_pollOut").empty().append(cur.pollOut);
        		$("#indexWarn_stocks").empty().append(cur.stocks);
        		$("#indexWarn_addDrop").empty().append(cur.addDrop);
        		$("#indexWarn_remote").empty().append(cur.remote);
        		$("#indexWarn_businessForChek").empty().append(cur.businessForChek);
        		$("#indexWarn_useTotal").empty().append(cur.useTotal);
        		$("#indexWarn_useMonthAdd").empty().append(cur.useMonthAdd);
        		$("#indexWarn_freeTotal").empty().append(cur.freeTotal);
        		$("#indexWarn_freeMonthAdd").empty().append(cur.freeMonthAdd);
        		
        		$("#maiteQulListCount").empty().append(cur.maiteQulListCount);
        		$("#perReminListCount").empty().append(cur.perReminListCount);
        		$("#enterQulListCount").empty().append(cur.enterQulListCount);
        		$("#postQulListCount").empty().append(cur.postQulListCount);
        		$("#policyQulListCount").empty().append(cur.policyQulListCount);


        	}           
        })
        //个人消息
        $.post("/emms/form/listBusinessMessage.do",{Q_receiveName_S_LK:parent.curUserInfo.fullname,limit:200,sort:"messageId",dir:"DESC"},function(res){
			var obj =eval('(' + res + ')');
			var html="";
			if(obj.success){
			$("#indexWarn_personTotal").empty().append(obj.totalCounts);
			for(var i=0;i<obj.totalCounts;i++){
				var cur=obj.result[i];
				var sor=i+1;
				var v1="<tr><td>"+sor+"、"+cur.message+"</td></tr>"
				html=html+v1;
			}
			$("#indexWarn_personList").empty().append(html);		
			}
        })
	});
function getTab(key){
	if(key=='leaveEquip'){
		parent.App.clickTopTab("EquipmentListView", {params:{Q_status_S_EQ :'1',fill_status_name :'闲置'}});
	}else if(key=='borrowEquip'){
		parent.App.clickTopTab("EquipmentListView", {params:{Q_status_S_EQ :'2',fill_status_name :'借用'}});
	}else if(key=='cancelEquip'){
		parent.App.clickTopTab("EquipmentListView", {params:{Q_status_S_EQ :'9',fill_status_name :'注销'}});
	}else if(key=='uselessEquip'){
		parent.App.clickTopTab("EquipmentListView", {params:{Q_status_S_EQ :'6',fill_status_name :'报废'}});
	}else if(key=='contract'){
		parent.App.clickTopTab("ContractLeaseListView", {params:{Q_applyforState_S_EQ :'3',fill_status_name :'计划中'}});
	}else if(key=='stockOut'){
		parent.App.clickTopTab("EquipmentListView", {params:{Q_businessStatus_S_EQ :'2',fill_status_name :'已出库'}});
	}else if(key=='logisOut'){
		parent.App.clickTopTab("LogisticsTransportListView", {params:{Q_status_S_EQ :'0',fill_status_name :'在途中'}});
	}else if(key=='logisSign'){
		parent.App.clickTopTab("LogisticsTransportListView", {params:{Q_status_S_EQ :'1',fill_status_name :'已签收'}});
	}else if(key=='installForCheck'){
		parent.App.clickTopTab("EquipInstallListView", {params:{Q_applyforState_S_EQ :'2',fill_status_name :'安装'}});
	}else if(key=='installForChecked'){
		parent.App.clickTopTab("EquipInstallListView", {params:{Q_applyforState_S_EQ :'3',fill_status_name :'待启用'}});
	}else if(key=='equipForDetect'){
		parent.App.clickTopTab("EquipDetectListView", {params:{fill_status_name :'检测'}});
	}else if(key=='equipForAccept'){
		parent.App.clickTopTab("EquipVerifyListView", {params:{fill_status_name :'验收'}});
	}else if(key=='equipForUse'){
		parent.App.clickTopTab("EquipActivateListView", {params:{Q_effective_S_EQ :'1',fill_status_name :'使用中'}});
	}else if(key=='equipForRepair'){
		parent.App.clickTopTab("EquipRepairListView", {params:{Q_status_S_EQ :'0',fill_status_name :'待维修'}});
	}else if(key=='equipForPoll'){
		parent.App.clickTopTab("EquipInspectEmployListView", {params:{Q_status_S_EQ :'0',fill_status_name :'待巡检'}});
	}else if(key=='equipForKeep'){
		parent.App.clickTopTab("EquipMaintEmployListView", {params:{Q_status_S_EQ :'0',fill_status_name :'待保养'}});
	}else if(key=='reform'){
		parent.App.clickTopTab("EquipInspectEmployListView", {params:{Q_inspectResult_S_EQ :'1',fill_status_name :'待整改'}});
	}else if(key=='equipForQuip'){
		parent.App.clickTopTab("EquipBlockupListView", {params:{Q_effective_S_EQ :'1',fill_status_name :'报停'}});
	}else if(key=='demount'){
		parent.App.clickTopTab("EquipDismantleListView", {params:{Q_applyforState_S_EQ :'3',fill_status_name :'拆卸'}});
	}else if(key=='cutTo'){
		parent.App.clickTopTab("EquipWarehouseListView", {params:{Q_applyforState_S_EQ :'2',fill_status_name :'转场维保'}});
	}else if(key=='putExcep'){
		parent.App.clickTopTab("EquipWarehouseAbnormalListView", {params:{fill_status_name :'入库异常'}});
	}else if(key=='useedEquip'){
	//	parent.App.clickTopTab("EquipWarehouseAbnormalListView", {params:{fill_status_name :'在用设备分布'}});
	}else if(key=='repairOut'){
		parent.App.clickTopTab("EquipRepairListView", {params:{fill_status_name :'待维修超期 '}});
	}else if(key=='pollOut'){
		parent.App.clickTopTab("EquipInspectEmployListView", {params:{fill_status_name :'待巡检超期'}});
	}else if(key=='stocks'){
		parent.App.clickTopTab("ComponetStocks", {params:{fill_status_name :'总资产及库存 '}});
	}else if(key=='addDrop'){
		parent.App.clickTopTab("KnotDispatch", {params:{fill_status_name :'加节/降节动态'}});
	}else if(key=='remote'){
	    window.open("http://www.rjtms.com/Map/MMap.aspx"); 
	}else if(key=='businessForChek'){
		show();
	//	parent.App.clickTopTab("EquipWarehouseAbnormalListView", {params:{fill_status_name :'待审业务'}});
	}
	
};
function show(){
	parent.App.clickTopTab("AppHomeShowView", {params:{fill_status_name :'待办事项'}});

};
