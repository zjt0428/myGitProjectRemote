<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.knight.emms.model.TemporaryReturn" table="T_TEMPORARY_RETURN" select-before-update="false" dynamic-update="false" dynamic-insert="false" optimistic-lock="version">
		<id name="returnId" column="RETURN_ID" type="java.lang.Long" length="19">
			<generator class="native" />
		</id>
		<property name="userId" type="java.lang.Long">
            <column name="USER_ID" not-null="true" />
        </property>
		<property name="userName" type="java.lang.String" column="USER_NAME" />
		<property name="returnSerial" type="java.lang.String" column="RETURN_SERIAL" />
		<property name="applyforState" type="java.lang.String" column="APPLYFOR_STATE" />
		<property name="licensePlate" type="java.lang.String" column="LICENSE_PLATE" />
		<property name="driver" type="java.lang.String" column="DRIVER" />
		<property name="depotId" type="java.lang.Long" column="DEPOT_ID" />
		<property name="depotName" type="java.lang.String" column="DEPOT_NAME" />
		<property name="projectId" type="java.lang.Long" column="PROJECT_ID" />
		<property name="projectName" type="java.lang.String" column="PROJECT_NAME" />
		<property name="applyDate" type="java.lang.String" column="APPLY_DATE" />
		<property name="packageDate" type="java.lang.String" column="PACKAGE_DATE" />
		<property name="deliveryMan" type="java.lang.String" column="DELIVERY_MAN" />
		<property name="affiliatedSerial" type="java.lang.String" column="AFFILIATED_SERIAL" />
		<property name="delFlag" type="java.lang.String" column="DEL_FLAG" />
		<property name="acceptTime" type="java.lang.String" formula="(SELECT TOP 1 T.ACCEPT_TIME FROM T_FORM_ACCEPT T WHERE T.RELATE_ID = RETURN_ID AND T.RELATE_MODULE='TEMPORARY_RETURN' ORDER BY T.ACCEPT_TIME DESC)" />
        <property name="approveTime" type="java.lang.String" formula="(SELECT TOP 1 T.APPROVE_TIME FROM T_FORM_APPROVE T WHERE T.RELATE_ID = RETURN_ID AND T.RELATE_MODULE='TEMPORARY_RETURN' ORDER BY T.APPROVE_TIME DESC)" />
        
		<many-to-one name="contractMaterials" column="CONTRACT_ID" class="com.knight.emms.model.ContractMaterials" fetch="join" />
		
		 <set name="temporaryReturnDetailSet" lazy="true" cascade="all" inverse="false" order-by="COMMODITY_ID DESC,SPECIFICATIONS_ID DESC">
			<key column="RETURN_ID"></key>
			<one-to-many class="com.knight.emms.model.TemporaryReturnDetail" />
		</set>
		<set name="chargeHandleSet" lazy="true" cascade="all" inverse="false">
			<key column="RETURN_ID"></key>
			<one-to-many class="com.knight.emms.model.ChargeHandle" />
		</set>
	</class>
</hibernate-mapping>
