	<queryString>
		<![CDATA[DECLARE @START_DATE smalldatetime
DECLARE @END_DATE smalldatetime
DECLARE @PROJECT_NAME VARCHAR(64);
DECLARE @PA_ENT_NAME VARCHAR(64);

SET @PROJECT_NAME = $P{PROJECT_NAME}
SET @START_DATE = CONVERT(VARCHAR(24),$P{START_DATE},102);
SET @END_DATE = CONVERT(VARCHAR(24),$P{END_DATE},102);

SELECT SE.BUILDING_NUM BUILDING_NUM,@START_DATE START_DATE,@END_DATE END_DATE,SC.PROJECT_NAME
FROM T_SETTLE_EQUIP_BRIEF SE
LEFT JOIN T_SETTLE_CONTRACT SC ON SC.SETTLE_ID = SE.SETTLE_ID
WHERE SE.MONTH_TAG IS NOT NULL
AND SC.PROJECT_NAME = @PROJECT_NAME]]>
	</queryString>
