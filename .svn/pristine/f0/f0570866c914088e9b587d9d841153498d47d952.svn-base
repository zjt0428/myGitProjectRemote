	<parameter name="PURPOSE_DATE_END" class="java.lang.String"/>
	<parameter name="GENERIC_NAME" class="java.lang.String"/>
	<parameter name="PURPOSE_DATE_BEG" class="java.lang.String"/>
	<parameter name="PURPOSE_NAME" class="java.lang.String"/>
	<queryString>
		<![CDATA[DECLARE @ym_beg CHAR(12)
DECLARE @ym_end CHAR(12)
DECLARE @ymds  CHAR(12)
DECLARE @ymde  CHAR(12)
DECLARE @GENERIC_NAME VARCHAR(64)
DECLARE @PURPOSE_NAME VARCHAR(64)

SET @ym_beg = $P{PURPOSE_DATE_BEG}
SET @ym_end = $P{PURPOSE_DATE_END}
SET @GENERIC_NAME = $P{GENERIC_NAME}
SET @PURPOSE_NAME = $P{PURPOSE_NAME}

SET @ymds = CONVERT(varchar(8),@ym_beg,112)
SET @ymde = CONVERT(varchar(8),@ym_end,112)
select * from
(select (
select BCG.VALUE from BM_COMPONENT_GENERIC BCG where a.COMPON_GENERIC =BCG.CODE ) COMPON_GENERIC ,(select BPP.VALUE from BM_PICKUP_PURPOSE BPP where b.PICKUP_PURPOSE =BPP.CODE ) PICKUP_PURPOSE ,SUM(a.QUANTITY) QUANTITY,a.UNIT_PRICE ,SUM(QUANTITY*UNIT_PRICE) TOTAL from T_PICKUP_COMPONENT a
left join T_PICKUP b on a.PICKUP_ID=b.PICKUP_ID
where b.PICKUP_DATE BETWEEN @ymds AND @ymde
group by COMPON_GENERIC,PICKUP_PURPOSE,a.UNIT_PRICE) AA
	   WHERE
(AA.COMPON_GENERIC LIKE '%'+@GENERIC_NAME+ '%' OR @GENERIC_NAME IS NULL OR @GENERIC_NAME = '')
AND (AA.PICKUP_PURPOSE LIKE '%'+@PURPOSE_NAME+ '%' OR @PURPOSE_NAME IS NULL OR @PURPOSE_NAME = '')]]>
	</queryString>
