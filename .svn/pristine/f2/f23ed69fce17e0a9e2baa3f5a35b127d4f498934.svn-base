<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.knight.emms.model.InsideBlockUpDetail" table="T_INSIDE_BLOCK_UP_DETAIL" select-before-update="false" dynamic-update="false" dynamic-insert="false"
		optimistic-lock="version">
		<id name="detailId" column="DETAIL_ID" type="java.lang.Long" length="19">
			<generator class="native" />
		</id>
		<property name="blockId" type="java.lang.Long" column="BLOCK_ID" />
		<property name="item" type="java.lang.String" column="ITEM" />
		<property name="amount" type="java.lang.String" column="AMOUNT" />
	</class>
</hibernate-mapping>
