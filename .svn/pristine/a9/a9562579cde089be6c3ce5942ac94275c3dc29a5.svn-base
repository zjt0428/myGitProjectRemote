<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.knight.emms.model.MaterialsRecycleCountTemp" table="T_MATERIALS_RECYCLE_COUNT_TEMP" select-before-update="false" dynamic-update="false" dynamic-insert="false"
		optimistic-lock="version">
		<id name="tempId" column="TEMP_ID" type="java.lang.Long" length="19">
			<generator class="native" />
		</id>
		<property name="specificationsId" type="java.lang.Long" column="SPECIFICATIONS_ID" ></property>
		<property name="recycleId" type="java.lang.Long" column="RECYCLE_ID" ></property>
		<property name="locationId" type="java.lang.Long" column="LOCATION_ID" ></property>
		<property name="quantity" type="java.lang.String" column="QUANTITY" ></property>
		<property name="locationName" type="java.lang.String" formula="(SELECT  T.LOCATION_NAME FROM T_BASE_LOCATION T WHERE T.LOCATION_ID = LOCATION_ID)" />
	</class>
</hibernate-mapping>
