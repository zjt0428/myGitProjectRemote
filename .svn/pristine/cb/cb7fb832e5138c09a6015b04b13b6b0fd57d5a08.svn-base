		<property name="driverPhone" type="java.lang.String" column="DRIVER_PHONE" />
		<property name="counts" type="java.lang.Integer" column="COUNTS" />
		<property name="signCounts" type="java.lang.Integer" column="SIGN_COUNTS" />
