<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.knight.emms.model.OtherBusiness" table="T_OTHER_BUSINESS" select-before-update="false" dynamic-update="false" dynamic-insert="false" optimistic-lock="version">
		<id name="otherBusinessId" column="OTHER_BUSINESS_ID" type="java.lang.Long" length="19">
			<generator class="native" />
		</id>
		<property name="applyforState" type="java.lang.String" column="APPLYFOR_STATE" />
		<property name="applyDate" type="java.lang.String" column="APPLY_DATE" />
		<property name="otherBusinessSerial" type="java.lang.String" column="OTHER_BUSINESS_SERIAL" />
		<property name="userId" type="java.lang.Long" column="USER_ID" />
		<property name="userName" type="java.lang.String" column="USER_NAME" />
		<property name="chargeableTime" type="java.lang.String" column="CHARGEABLE_TIME" />
		<property name="contractId" type="java.lang.Long" column="CONTRACT_ID" />
		<property name="contractSerial" type="java.lang.String" column="CONTRACT_SERIAL"/>
		<property name="projectName" type="java.lang.String" column="PROJECT_NAME" />
		<property name="paEntName" type="java.lang.String" column="PA_ENT_NAME" />
		<property name="totalCosts" type="java.lang.String" column="TOTAL_COSTS" />
		<property name="affiliatedSerial" type="java.lang.String" column="AFFILIATED_SERIAL" />
		<property name="contractNumber" type="java.lang.String" column="CONTRACT_NUMBER" />
		<property name="remark" type="java.lang.String" column="REMARK" />
		
		<property name="delFlag" type="java.lang.String" column="DEL_FLAG" />
		<many-to-one name="baseDepot" column="DEPOT_ID" class="com.knight.emms.model.BaseDepot" fetch="join" />
		<set name="otherBusinessDetailSet" cascade="all" inverse="false" lazy="true">
			<key column="OTHER_BUSINESS_ID" />
			<one-to-many class="com.knight.emms.model.OtherBusinessDetail" />
		</set>
	</class>
</hibernate-mapping>