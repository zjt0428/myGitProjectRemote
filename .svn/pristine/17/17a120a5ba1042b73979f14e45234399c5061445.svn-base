.fl{ float: left}
.fr{ float: right}
.white14{ font-size:14px;	color:#DAEDF9; text-align:right;}
.t_center{ text-align:center !important;}
.t_right{ text-align:center !important;}


a{ text-decoration:none;}
a:link, a:visited{ color:#000000;}
a:hover{ color:#FF6D00; text-decoration:underline;}

.tbold{ font-weight:bold;}
.wrod_title{font-size:24px;font-family: "宋体"; font-weight:bold; line-height:32px; text-align:center; padding-bottom:10px;}
.botton_line{border-bottom:#000000 1px solid; padding-left:10px;font-family: "黑体"; font-size:20pt;}
.blue_bold{ color:#0000FF; font-weight:bold;}
