/*列表页表格*/
.listtable{
	width:100%;	
	font-size:14px;	
	border: 1px solid #000000; 
	border-collapse: collapse;}
	
.listtable th {
	border-left:1px solid #000000;
	border-bottom:1px solid #000000;
	color:#000000; 
	padding-left:3px;
	font-weight:bold;
	text-align:center;
	}
	
.listtable td {
	border-left:1px solid #000000;
	border-bottom:1px solid #000000;
	color:#000000;
	line-height:20px;
	padding-top:5px;
	padding-left:5px;
	}		

/*没边线表格*/
.list_none{width:100%;font-size:14px; border:none;}
.list_none td {color:#000000;line-height:36px;padding-left:3px; border:none;}	

