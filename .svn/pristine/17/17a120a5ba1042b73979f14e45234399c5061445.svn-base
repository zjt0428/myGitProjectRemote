body {
	color:#000000;
	margin:0 auto;
	font-size:14px;
   }
   
div,table,select,textarea,form,p,h1,h2,h3,h4,h5,fieldset,p{padding:0; margin:0;}
ul,li,dl,dt,dd{padding:0; margin:0; list-style:none;}
