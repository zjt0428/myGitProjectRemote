<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.knight.app.model.AttendamceLocation" table="T_ATTENDAMCE_LOCATION"
		select-before-update="false" dynamic-update="false" dynamic-insert="false"
		optimistic-lock="version">
		<id name="lid" column="LID" type="java.lang.Long" length="19">
			<generator class="native" />
		</id>
		<property name="aid" type="java.lang.Long" column="AID" />
		<property name="upTime" type="java.lang.String" column="UPTIME" />
		<property name="location" type="java.lang.String" column="LOCATION" />
	</class>
</hibernate-mapping>
