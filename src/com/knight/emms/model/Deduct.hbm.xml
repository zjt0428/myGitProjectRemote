		<property name="contractSerial" type="java.lang.String" column="CONTRACT_SERIAL" />
		<property name="contractTheme" type="java.lang.String" column="CONTRACT_THEME" />
		<property name="contractAmount" type="java.math.BigDecimal" column="CONTRACT_AMOUNT" />
		<property name="disbursement" type="java.math.BigDecimal" column="DISBURSEMENT" />
		<property name="proportion" type="java.math.BigDecimal" column="PROPORTION" />
		<property name="cardinal" type="java.lang.String" column="CARDINAL" />
		<property name="proportionType" type="java.lang.String" column="PROPORTION_TYPE" />
