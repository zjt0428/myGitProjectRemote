			<generator class="native" />
		</id>
		<property name="scaleStart" type="java.math.BigDecimal" column="SCALE_START" />
		<property name="scaleEnd" type="java.math.BigDecimal" column="SCALE_END" />
		<property name="scalePercent" type="java.math.BigDecimal" column="SCALE_PERCENT" />
