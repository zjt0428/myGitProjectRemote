		<property name="opinionUsername" type="java.lang.String" column="OPINION_USERNAME" />
		<property name="opinionDep" type="java.lang.String" column="OPINION_DEP" />
		<property name="opinionTime" type="java.util.Date" column="OPINION_TIME" />
		<property name="opinionContent" type="java.lang.String" column="OPINION_CONTENT" />
		<property name="opinionRemark" type="java.lang.String" column="OPINION_REMARK" />
	</class>
</hibernate-mapping>
