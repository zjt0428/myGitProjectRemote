	<queryString>
		<![CDATA[DECLARE @ym_beg CHAR(12)
DECLARE @ym_end CHAR(12)
DECLARE @ymds  CHAR(12)
DECLARE @ymde  CHAR(12)
DECLARE @PURCHASER_NAME VARCHAR(64)
DECLARE @BRIEF_NAME VARCHAR(64)
DECLARE @SUPPLIER_NAME VARCHAR(64)
DECLARE @SPECIFIC VARCHAR(64)

SET @ym_beg = $P{PURCHASE_DATE_BEG}
SET @ym_end = $P{PURCHASE_DATE_END}
SET @BRIEF_NAME = $P{BRIEF_NAME}
SET @PURCHASER_NAME = $P{PURCHASER_NAME}
SET @SUPPLIER_NAME = $P{SUPPLIER_NAME}
SET @SPECIFIC = $P{SPECIFIC}

SET @ymds = CONVERT(varchar(8),@ym_beg,112)
SET @ymde = CONVERT(varchar(8),@ym_end,112)+1
select * from(
select PURCHASE_DATE, a.SUPPLIER_NAME, PURCHASER_NAME, BRIEF_NAME, SPECIFIC, DIMENSIONS, QUANTITY, UNIT_PRICE, SUMMARY, a.PURCHASE_AMOUNT, a.APPLYFOR_STATE
from T_PURCHASE a left join T_PURCHASE_BRIEF b on a.PURCHASE_ID=b.PURCHASE_ID) AA
	   WHERE
	     (AA.BRIEF_NAME LIKE '%'+@BRIEF_NAME+ '%' OR @BRIEF_NAME IS NULL OR @BRIEF_NAME = '')
	   AND (AA.SUPPLIER_NAME LIKE '%'+@SUPPLIER_NAME+ '%' OR @SUPPLIER_NAME IS NULL OR @SUPPLIER_NAME = '')
	   AND (AA.PURCHASER_NAME LIKE '%'+@PURCHASER_NAME+ '%' OR @PURCHASER_NAME IS NULL OR @PURCHASER_NAME = '')
	   AND (AA.SPECIFIC LIKE '%'+@SPECIFIC+ '%' OR @SPECIFIC IS NULL OR @SPECIFIC = '')
	   AND (AA.PURCHASE_DATE >= @ymds AND AA.PURCHASE_DATE <= @ymde)
	   AND (AA.APPLYFOR_STATE != 0 AND AA.APPLYFOR_STATE != 1 AND AA.APPLYFOR_STATE !=2)]]>
	</queryString>
