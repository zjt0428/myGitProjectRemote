	<queryString>
		<![CDATA[SELECT CL.PA_ENT_NAME,P.PROJECT_NAME,P.PROJECT_ID,P.LEASE_PROJECT_HEAD,CL.COMPETENT_DEPARTMET,T.*
FROM (
	SELECT CONTRACT_ID,
	SUM(SETTLE_AMOUNT) AS 'SETTLE_AMOUNT',
	SUM(FINISHED_AMOUNT) AS 'FINISHED_AMOUNT',
	SUM(BALANCE_AMOUNT) AS 'BALANCE_AMOUNT'
	FROM T_SETTLE_CONTRACT WHERE EFFECTIVE='1' GROUP BY CONTRACT_ID ) T
LEFT JOIN T_CONTRACT_LEASE CL ON CL.CONTRACT_ID=T.CONTRACT_ID
LEFT JOIN T_PROJECT P ON P.PROJECT_ID=CL.PROJECT_ID]]>
	</queryString>
