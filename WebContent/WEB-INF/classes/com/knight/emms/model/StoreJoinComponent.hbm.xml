<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.knight.emms.model.StoreJoinComponent" table="T_STORE_JOIN_COMPONENT" select-before-update="false" dynamic-update="false" dynamic-insert="false"
		optimistic-lock="version">
		<id name="joinId" column="JOIN_ID" type="java.lang.Long" length="19">
			<generator class="native" />
		</id>
		<property name="counts" type="java.lang.Double" column="COUNTS" />
		<many-to-one name="storeHouse" column="STORE_ID" class="com.knight.emms.model.StoreHouse" fetch="join" />
		<many-to-one name="component" column="COMPON_ID" class="com.knight.emms.model.Component" fetch="join" />
		
	</class>
</hibernate-mapping>
