			<generator class="native" />
		</id>
		<property name="reimburseId" type="java.lang.Long" column="REIMBURSE_ID" />
		<property name="reimburseType" type="java.lang.String" column="REIMBURSE_TYPE" />
		<property name="ticketDate" type="java.lang.String" column="TICKET_DATE" />
		<property name="ticketQuantity" type="java.lang.Integer" column="TICKET_QUANTITY" />
		<property name="specificName" type="java.lang.String" column="SPECIFIC_NAME" />
		<property name="modelName" type="java.lang.String" column="MODEL_NAME" />
		<property name="unitPrice" type="java.math.BigDecimal" column="UNIT_PRICE" />
		<property name="quantity" type="java.lang.Integer" column="QUANTITY" />
		<property name="summary" type="java.math.BigDecimal" column="SUMMARY" />
		<property name="remark" type="java.lang.String" column="REMARK" />
		<property name="carId" type="java.lang.Long" column="CAR_ID" />
		<property name="licensePlate" type="java.lang.String" column="LICENSE_PLATE" />
	</class>
</hibernate-mapping>
