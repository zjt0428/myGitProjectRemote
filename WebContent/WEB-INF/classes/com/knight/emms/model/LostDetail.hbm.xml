		<property name="projectComponId" type="java.lang.Long" column="PROJECT_COMPON_ID" />
		<property name="counts" type="java.lang.String" column="COUNTS" />
		<property name="lostCounts" type="java.lang.String" column="LOST_COUNTS" />
		<property name="lostCost" type="java.lang.String" column="LOST_COSTS" />
		<property name="damageCounts" type="java.lang.String" column="DAMAGE_COUNTS" />
		<property name="damageCosts" type="java.lang.String" column="DAMAGE_COSTS" />
		<property name="totals" type="java.lang.String" column="TOTALS" />
		<property name="describe" type="java.lang.String" column="DESCRIBE" />
		<property name="loseDescribe" type="java.lang.String" column="LOSE_DESCRIBE" />
		<property name="calculate" type="java.lang.String" column="CALCULATE" />
		<many-to-one name="component" column="COMPON_ID" class="com.knight.emms.model.Component" insert="false" update="false" fetch="join" />
	</class>
</hibernate-mapping>
